{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.config.ts",
    "build:ssr": "tsc && vite build --config ssr.config.ts",
    "preview": "vite preview",
    "eslint:check": "eslint --quiet --ext .ts,.tsx src --cache --cache-location=./.eslint-cache",
    "prettier:check": "prettier --check ./",
    "lint": "yarn run eslint:check && yarn run prettier:check",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "prettier": "prettier --write .",
    "format": "npm run lint:fix && npm run prettier",
    "test": "jest ./",
    "swagger": "swagger-typescript-api --axios --responses --extract-enums --extract-request-params --extract-request-body -p ./src/shared/api/swagger.json -o ./src/shared/api -n swagger.ts"
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@testing-library/user-event": "^14.4.3",
    "antd": "5.1.0",
    "axios": "1.2.1",
    "dotenv": "16.0.2",
    "effector": "22.4.1",
    "effector-react": "22.4.0",
    "effector-storage": "^5.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.11",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-media-hook": "0.5.0",
    "react-router-dom": "6.6.0",
    "styled-components": "5.3.6",
    "swagger-typescript-api": "^12.0.2",
    "vite-tsconfig-paths": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "28",
    "jest-environment-jsdom": "29.0.1",
    "lefthook": "1.1.1",
    "msw": "^1.1.0",
    "prettier": "2.7.1",
    "rimraf": "^4.1.2",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2",
    "vite": "3.0.7"
  },
  "license": "MIT"
}
